{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch to Chrome",
            "url": "http://localhost:1338",
            "request": "launch",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Attach Editor",
            "type": "chrome",
            "request": "attach",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "port": 8315
        },
        {
            "name": "Attach to My Browser",
            "type": "chrome",
            "request": "attach",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "port": 1338
        }
    ]
}